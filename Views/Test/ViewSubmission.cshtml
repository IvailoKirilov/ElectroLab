@model IEnumerable<ElectroLab.Models.Submission>

@{
    ViewData["Title"] = "My Submissions";
}

<h2>My Submissions</h2>

<table class="table">
    <thead>
        <tr>
            <th>Test Title</th>
            <th>Score</th>
            <th>Date Submitted</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var submission in Model)
        {
            <tr>
                <td>@submission.Test.Title</td>
                <td>@submission.Score</td>
                <td>@submission.DateSubmitted</td>
            </tr>
        }
    </tbody>
</table>
